{
  "name": "hellobooks",
  "version": "1.0.2",
  "description": "Hello-Books is a simple application that helps manage a library and its processes like stocking, tracking and renting books.",
  "scripts": {
    "test": "mocha",
    "start": "nodemon ./server/bin/www",
    "clean": "rm -rf dist",
    "start": "node bin/dev",
    "build": "npm run clean && mkdir dist && babel server -d dist",
    "production": "npm run build && node bin/production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dbrandydude/hellobooks.git"
  },
  "keywords": [
    "hellobooks",
    "library",
    "book"
  ],
  "author": "Harry Dbrandy",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dbrandydude/hellobooks/issues"
  },
  "homepage": "https://github.com/dbrandydude/hellobooks#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "~1.17.1",
    "connect-flash": "^0.1.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "dotenv": "^4.0.0",
    "ejs": "~2.5.6",
    "express": "~4.15.2",
    "express-session": "^1.15.4",
    "morgan": "~1.8.1",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "pg": "^7.1.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.4.2",
    "serve-favicon": "~2.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.1.0",
    "chai-http": "^3.0.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "mocha-eslint": "^4.1.0"
  }
